<tr id="{{id}}" class="tr">
{{#each values}}
  <td class="td {{this.class}}" {{#if this.title}}title="{{this.title}}"{{/if}}>{{#if this.value.html}}<span {{#if this.title}}title="{{this.title}}"{{/if}}>{{#if this.href}}<a href="#{{this.href}}">{{/if}}{{{this.value.html}}}</span>{{#if this.href}}</a>{{/if}}{{/if}}
{{/each}}
</tr>
